#!/bin/bash
#
# skripti algus
#
echo -n "Sisesta aluskandiku pikkus(cm):"
read k_pikkus
echo -n "Sisesta aluskandiku laius(cm):"
read k_laius
echo -n "Kui suuri k체psiseid kasutatakse pikkus(cm)?: "
read ky_pikkus
echo -n "K체psise laius(cm):"
read ky_laius
echo -n "Mitu kihti teete?: "
read kiht
echo -n "Mitu k체psist on pakis?: "
read pakis
echo "Poest tuleb osta" `expr  $k_pikkus \* $k_laius  / $ky_pikkus \* $ky_laius \* $kiht / $pakis` "pakki"
#
# skripti l천pp
