#!/bin/bash
#
# skripti algus
#
echo -n "Mitu reisijat on grupis: "
read reisijad
echo -n "Mitu kohta on bussis? "
read kohad
kokku=`expr $kohad - $reisija`
taidetud=`expr $reisijad / $kohad`
echo "T채ielikult t채idetud busse on" `expr $taidetud` "maha j채i" `expr $kokku` "inimest"
#
# skripti l천pp
 

